{
  "name": "online-survey-api",
  "version": "1.0.0",
  "description": "Backend API for online survey application",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:migrate-users": "tsx src/db/add-users-migration.ts",
    "db:migrate-respondent-age": "tsx src/db/add-respondent-age-migration.ts",
    "db:migrate-password": "tsx src/db/add-password-migration.ts"
  },
  "keywords": ["survey", "api", "express", "postgresql"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "zod": "^3.22.4",
    "uuid": "^9.0.1",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "typescript": "^5.3.3",
    "@types/node": "^20.10.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/pg": "^8.10.9",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^9.0.7",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "tsx": "^4.7.0"
  }
}
